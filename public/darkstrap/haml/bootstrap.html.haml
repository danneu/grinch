!!! 5
%html
  %head
    %title Bootstrap
    %link(href="stylesheets/bootstrap.css" rel="stylesheet")
    %link(href="stylesheets/bootstrap.responsive" rel="stylesheet")
  %body
    .navbar
      .navbar-inner
        .container
          %a.brand(href="/") Bootstrap
          %ul.nav
            %li 
              %a.active(href="/") About
            %li 
              %a(href="/") Contact
    .container
      .content
        %ul.breadcrumb
          %li
            %a(href="/") Home
            %span.divider /
          %li 
            %a(href="/") Crumb
            %span.divider /
          %li Crumb


        .page-header
          %h1 
            Bootstrap

        .well
          %a.btn.btn-warning(href="darkstrap.html")
            Toggle Darkstrap
          %a.btn.btn-default(href="bootstrap.html")
            Toggle Bootstrap
          %ul(style="margin-top: 10px; margin-bottom: 0;")
            %li
              Download
              %a(href="https://raw.github.com/danneu/darkstrap/master/stylesheets/darkstrap.css") darkstrap.css
            %li 
              Simply include it 
              %em after
              bootstrap's css

        %h1 Header 1
        %h2 Header 2
        %h3 Header 3
        %h4 Header 4
        %h5 Header 5
        %h6 Header 6

        %hr/
        
        %h1 Give anything the .well class

        %p.well
          Becomes radical.

        %hr/

        %h1 Blockquotes

        .row
          .span12
            %blockquote
              %p Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere erat a ante venenatis.
              %small Someone Famous
            %blockquote.pull-right
              %p Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer posuere erat a ante venenatis.
              %small Someone Famous

        %hr/

        %h1 Lists

        .row
          .span3
            %h4 Unordered List
            %ul
              %li Item 1
              %li Item 2
              %li Item 3
          .span3
            %h4 Unordered, Unstyled List
            %ul.unstyled
              %li Item 1
              %li Item 2
              %li Item 3
          .span3
            %h4 Ordered List
            %ol
              %li Item 1
              %li Item 2
              %li Item 3
          .span3
            %h4 Description List
            %dl
              %dt Something
              %dd Somthing
              %dt Something
              %dd Somthing
              %dt Something
              %dd Somthing

        %hr/

        %h1 Code

        .row
          .span8.offset2
            %p
              Here's an 
              %code inline
              snippet.

            %pre
              :preserve
                class BasicBlock
                  puts "Here's a basic block"
                end

        %hr/

        %h1 Tables
        .row
          .span8.offset2
            %table.table
              %thead
                %tr
                  %th #
                  %th First Name
                  %th Last Name
              %tbody
                %tr
                  %td 1
                  %td Chuck
                  %td Bancroft
                %tr
                  %td 2
                  %td Hilary
                  %td Zezu
                %tr
                  %td 3
                  %td Billy Bob
                  %td Bobblesmith
        .row
          .span8.offset2
            %table.table.table-striped
              %thead
                %tr
                  %th #
                  %th First Name
                  %th Last Name
              %tbody
                %tr
                  %td 1
                  %td Chuck
                  %td Bancroft
                %tr
                  %td 2
                  %td Hilary
                  %td Zezu
                %tr
                  %td 3
                  %td Billy Bob
                  %td Bobblesmith
        .row
          .span8.offset2
            %table.table.table-bordered
              %thead
                %tr
                  %th #
                  %th First Name
                  %th Last Name
              %tbody
                %tr
                  %td 1
                  %td Chuck
                  %td Bancroft
                %tr
                  %td 2
                  %td Hilary
                  %td Zezu
                %tr
                  %td 3
                  %td Billy Bob
                  %td Bobblesmith
        .row
          .span8.offset2
            %table.table.table-condensed
              %thead
                %tr
                  %th #
                  %th First Name
                  %th Last Name
              %tbody
                %tr
                  %td 1
                  %td Chuck
                  %td Bancroft
                %tr
                  %td 2
                  %td Hilary
                  %td Zezu
                %tr
                  %td 3
                  %td Billy Bob
                  %td Bobblesmith
        %hr/

        %h1 Forms

        .row
          .span8.offset2
            %form.well
              %label Label name
              %input(type="text" placeholder="Type something")
                %span.help-inline Associated help text!
              %label.checkbox
                %input(type="checkbox") Check me out
              %button.btn(type="submit") Submit
        .row
          .span8.offset2
            %form.well.form-search
              %input.search-query(type="text")
              %button.btn(type="submit") Search
        .row
          .span8.offset2
            %form.well.form-inline
              %input.input-small(type="text" placeholder="Email")
              %input.input-small(type="password" placeholder="Password")
              %button.btn(type="submit") Go

        .row
          .span8.offset2
            %form.form-horizontal
              %fieldset
                %legend Here's a legend
                .control-group
                  %label.control-label(for="input01") Text input
                  .controls
                    %input.input-xlarge#input01(type="text")
                    %p.help-block Here's a help block
                .control-group
                  %label.control-label(for="optionsCheckbox") Checkbox
                  .controls
                    %label.checkbox
                      %input#optionsCheckbox(type="checkbox" value="options1")
                        Option one is this and that.

                .control-group
                  %label.control-label(for="select01") Select list
                  .controls
                    %select#select01
                      %option something
                      %option 2
                      %option 3
                      %option 4
                      %option 5

                .control-group
                  %label.control-label(for="multiSelect") Multi select
                  .controls
                    %select#multiSelect(multiple="multiple")
                      %option something
                      %option 2
                      %option 3
                      %option 4
                      %option 5

                .control-group
                  %label.control-label(for="fileInput") File input
                  .controls
                    %input.input-file#fileInput(type="file")

                .control-group
                  %label.control-label(for="textarea") Textarea
                  .controls
                    %textarea#textarea.input-xlarge(rows="3")

                .form-actions
                  %button.btn.btn-primary(type="submit") Save changes
                  %button.btn(type="reset") Cancel


        .row
          .span8.offset2
            %form.form-horizontal
              %fieldset
                %legend Form control states
                .control-group.warning
                  %label.control-label(for="inputError") Input with warning
                  .controls
                    %input#inputError(type="text")
                    %span.help-inline Something may have gone wrong
                .control-group.error
                  %label.control-label(for="inputError") Input with error
                  .controls
                    %input#inputError(type="text")
                    %span.help-inline Something did go wrong
                .control-group.success
                  %label.control-label(for="inputError") Input with error
                  .controls
                    %input#inputError(type="text")
                    %span.help-inline It went right!

                .control-group.success
                  %label.control-label(for="selectError") Select with success
                  .controls
                    %select#selectError
                      %option 1
                      %option 2
                      %option 3
                      %option 4
                      %option 5
                    %span.help-inline This also went right!

            .form-actions
              %button.btn.btn-primary(type="submit") Save changes
              %button.btn(type="reset") Cancel

        %hr/

        %h1 Buttons

        .row
          .span8.offset2
            %button.btn Default
            %button.btn.btn-primary Primary
            %button.btn.btn-info Info
            %button.btn.btn-success Success
            %button.btn.btn-warning Warning
            %button.btn.btn-danger Danger



      %footer
        %hr
        %p
          Darkstrap on 
          %a(href="https://github.com/danneu/darkstrap") Github

  
